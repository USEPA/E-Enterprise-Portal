{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.14.6",
    "vue": "^2.5.22",
    "vue-cookie": "^1.1.4",
    "vue-grid-layout": "^2.3.3",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",
    "xml-js": "^1.6.9"
  },
  "devDependencies": {
    "@vue/cli": "^3.3.0",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-plugin-unit-mocha": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.28",
    "chai": "^4.2.0",
    "eslint": "^5.12.1",
    "eslint-config-fixable-all": "^5.10.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_attributes"
          ]
        }
      ],
      "vue/html-self-closing": [
        "error",
        {
          "html": {
            "void": "never",
            "normal": "always",
            "component": "always"
          },
          "svg": "always",
          "math": "always"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off"
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}

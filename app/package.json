{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.15",
    "core-js": "^2.6.5",
    "element-resize-detector": "^1.1.15",
    "interactjs": "^1.3.4",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.14.7",
    "uswds": "^2.0.0-beta.6",
    "vue": "^2.6.9",
    "vue-analytics": "^5.16.4",
    "vue-cookie": "^1.1.4",
    "vue-gtm": "^2.0.0",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.15.0",
    "vuex": "^3.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@vue/cli": "^3.5.1",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-unit-mocha": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "@vue/eslint-config-airbnb": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-plugin-transform-flow-comments": "^6.7.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.3",
    "eslint-config-fixable-all": "^5.10.0",
    "node-sass": "^4.11.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.9",
    "webpack": "^4.29.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "no-mixed-operators": [
        "error",
        {
          "groups": [
            [
              "+",
              "-",
              "*",
              "/",
              "%",
              "**"
            ],
            [
              "&",
              "|",
              "^",
              "~",
              "<<",
              ">>",
              ">>>"
            ],
            [
              "==",
              "!=",
              "===",
              "!==",
              ">",
              ">=",
              "<",
              "<="
            ],
            [
              "&&",
              "||"
            ],
            [
              "in",
              "instanceof"
            ]
          ],
          "allowSamePrecedence": true
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_attributes"
          ]
        }
      ],
      "vue/html-self-closing": [
        "error",
        {
          "html": {
            "void": "never",
            "normal": "always",
            "component": "always"
          },
          "svg": "always",
          "math": "always"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "*.vue"
        ],
        "rules": {
          "indent": "off"
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}

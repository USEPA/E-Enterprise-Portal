<?php

/**
 * @file
 * Contains eep_permit_search.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function eep_permit_search_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the eep_permit_search module.
    case 'help.page.eep_permit_search':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Permit Search proxy service endpoint. Query ?docs for documentation.') . '</p>';
      return $output;

    default:
  }
}

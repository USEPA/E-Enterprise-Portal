<?php

/**
 * @file
 * Contains eep_my_certifications.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 * @param $route_name
 * @param $route_match
 *
 * @return $output
 *
 */
function eep_my_certifications_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.eep_my_certifications':
      $output = '';
      $output .= '<h1>' . t('EEP My Certifications Help Page') . '</h1>';
      return $output;
    default:
      return "";
  }
}
